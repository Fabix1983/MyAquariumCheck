<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MyAquariumCheck.TabPage"
             xmlns:local="clr-namespace:MyAquariumCheck"
             Title="TabbedPageDemo"
             BarBackgroundColor="mediumturquoise"
             BarTextColor="White"
             SelectedTabColor="White"
             UnselectedTabColor="Black"
             xmlns:android="clr-namespace:Microsoft.Maui.Controls.PlatformConfiguration.AndroidSpecific;assembly=Microsoft.Maui.Controls"
             android:TabbedPage.ToolbarPlacement="Bottom">
    
    <local:MainPage Title="Home" IconImageSource="home.svg">
    </local:MainPage>
    <local:NewCheck Title="New" IconImageSource="add.svg">
    </local:NewCheck>
    <local:ChartPage Title="Chart" IconImageSource="chart.svg">
    </local:ChartPage>
    <local:InfoApp Title="Info" IconImageSource="infoicon.svg">
    </local:InfoApp>
    <local:ListPage Title="List" IconImageSource="list.svg">
    </local:ListPage>

</TabbedPage>